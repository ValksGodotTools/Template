{
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "files.insertFinalNewline": true,
    "files.exclude": {
        "**/.gitattributes": true,
        "**/.gitignore": true,
        "**/.gitmodules": true,
        "**/.runsettings": true,
        "**/project.godot": true,
        "**/.vs": true,
        "**/*.cfg": true,
        "**/*.cmd": true,
        "**/*.import": true,
        "**/*.sln": true,
        "**/*.txt": true,
        "**/bin": true,
        "**/obj": true,
        "**/LICENSE": true,
        "**/README.md": true
    },
    "cscurlyformatter.autoTabIndentation": false,
    "csharpextensions.useFileScopedNamespace": true,
    "csharpextensions.privateMemberPrefix": "_",
}
